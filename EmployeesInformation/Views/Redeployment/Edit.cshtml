@Model Redeployment

<table>
    <tr>
        <th>Id</th>
        <th>Employee Id</th>
        <th>From Department Id</th>
        <th>To Department Id</th>
        <th>From Position Id</th>
        <th>To Position Id</th>
        <th>Time</th>
    </tr>
    <tr>
        <th><p>@Model.Id</p></th>
        <th><p>@Model.EmployeeID</p></th>
        <th><p>@Model.FromDepartmentID</p></th>
        <th><p>@Model.ToDepartmentID</p></th>
        <th><p>@Model.FromPositionID</p></th>
        <th><p>@Model.ToPositionID</p></th>
        <th><p>@Model.Time</p></th>
    </tr>
</table>

@using (Html.BeginForm("Edit", "Redeployment", FormMethod.Post))
{
<fieldset>
    <p>
        <label>Enter Employee Id:</label><br />
        <input type="text" name="EmployeeID" />
    </p>
    <p>
        <label>Enter From Department Id:</label><br />
        <input type="text" name="FromDepartmentID" />
    </p>
    <p>
        <label>Enter To Department Id:</label><br />
        <input type="text" name="ToDepartmentID" />
    </p>
    <p>
        <label>Enter FromPosition Id:</label><br />
        <input type="text" name="FromPositionID" />
    </p>
    <p>
        <label>Enter To PositionId:</label><br />
        <input type="text" name="ToPositionID" />
    </p>
    <p>
        <label>Enter time:</label><br />
        <input type="text" name="Time" />
    </p>
    <p>
        <input type="submit" value="Save changes" />
    </p>
</fieldset>
}
